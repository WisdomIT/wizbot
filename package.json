{
  "name": "wizbot",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "workspaces/*"
  ],
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm -r run dev",
    "build": "pnpm build:api && pnpm -r run build",
    "build:api": "pnpm --filter api start & sleep 5 && pnpm --filter api stop",
    "build:withoutapi": "pnpm -r run build",
    "start": "pm2 start ecosystem.config.js",
    "list": "pm2 list",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "lint": "pnpm -r run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "dotenv -e apps/api/.env -- prisma generate --schema=apps/api/prisma/schema.prisma",
    "prisma:migrate": "dotenv -e apps/api/.env -- prisma migrate dev --schema=apps/api/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.5.2",
  "devDependencies": {
    "@wizbot/eslint-config": "workspace:^",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.5.0",
    "typescript": "^5.8.2"
  }
}
